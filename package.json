{
  "name": "prilabsa-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Prilabsa Corporate Website 2025 - Modern React TypeScript Application",
  "keywords": [
    "prilabsa",
    "corporate-website",
    "aquaculture",
    "ecuador",
    "react",
    "typescript",
    "vite",
    "tailwindcss"
  ],
  "author": {
    "name": "Prilabsa",
    "email": "info@prilabsa.com",
    "url": "https://prilabsa.com"
  },
  "homepage": "https://prilabsa.com",
  "license": "UNLICENSED",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "validate:images": "node scripts/validate-images-integrity.js",
    "build": "node scripts/materialize-expected-images.cjs && node scripts/copy-product-assets.cjs && tsx scripts/verify-products-integrity.js && node scripts/validate-images-integrity.js && vite build && node scripts/post-build-verify.js",
    "build:clean": "rm -rf dist && rm -rf .vite",
    "build:fast": "node scripts/materialize-expected-images.cjs && vite build --mode production",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "preview": "npm run build && vite preview",
    "test": "npx vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run --reporter=verbose --silent",
    "test:ci": "vitest run --reporter=verbose",
    "test:coverage": "vitest --coverage --reporter=verbose",
    "test:watch": "vitest --watch --reporter=verbose",
    "lint": "eslint src --ext ts,tsx --quiet",
    "lint:fix": "eslint . --ext ts,tsx --fix --quiet",
    "type-check": "tsc --noEmit --incremental",
    "verify-products": "tsx scripts/verify-products-integrity.js",
    "type-check:watch": "tsc --noEmit --watch",
    "audit:security": "npm audit --audit-level critical --json",
    "audit:fix": "npm audit fix",
    "compose:report": "tsx scripts/composeReport.ts",
    "predeploy": "npm run type-check && npm run build",
    "predeploy:full": "npm run test:run && npm run lint && npm run build",
    "translations:validate": "node scripts/validate-translations.cjs",
    "translations:status": "node scripts/validate-translations.cjs --format=status",
    "translations:deploy": "npm run translations:validate && npm run build && npm run deploy",
    "translations:test": "npm run test -- --grep=translation",
    "translations:verify": "node scripts/verify-105-products.cjs",
    "deploy": "wrangler pages deploy dist",
    "deploy:ftp": "tsx scripts/deploy-ftp.js",
    "deploy:ftp:fast": "npm run build:fast && tsx scripts/deploy-ftp.js",
    "deploy:ftp:full": "npm run predeploy:full && tsx scripts/deploy-ftp.js",
    "test:ftp": "node scripts/test-ftp-connection.js",
    "test:deployment": "node scripts/test-github-actions-deployment.js",
    "ssl:fix": "node scripts/fix-cloudflare-ssl.js",
    "ssl:check": "./scripts/check-ssl-status.sh",
    "dns:fix": "node scripts/fix-dns-redirect.js",
    "dns:verify": "node scripts/verify-dns-fix.js",
    "dns:auto-fix": "node scripts/cloudflare-dns-fix.js",
    "dns:monitor": "node scripts/monitor-dns-realtime.js",
    "dns:diagnose": "node scripts/diagnose-cloudflare-conflict.cjs",
    "deploy:staging": "node scripts/deploy-staging.js",
    "deploy:staging:fast": "npm run build:fast && node scripts/deploy-staging.js",
    "deploy:staging:full": "npm run predeploy:full && node scripts/deploy-staging.js",
    "setup:staging": "chmod +x scripts/setup-staging-ssh.sh && scripts/setup-staging-ssh.sh",
    "verify:staging": "node scripts/verify-staging-setup.js",
    "setup:credentials": "node scripts/setup-credentials.js",
    "verify:wordpress": "node scripts/verify-wordpress-setup.js",
    "deploy:wordpress": "node scripts/deploy-wordpress-vite.js",
    "deploy:wordpress:fast": "npm run build:fast && VITE_DEPLOY_TARGET=staging node scripts/deploy-wordpress-vite.js",
    "deploy:wordpress:full": "npm run predeploy:full && VITE_DEPLOY_TARGET=staging node scripts/deploy-wordpress-vite.js",
    "clean": "rm -rf dist coverage node_modules/.cache .vite",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "optimize": "npm run clean && npm run type-check && npm run test:run && npm run build",
    "deploy:fast": "npm run type-check && npm run build:fast",
    "analyze-bundle": "npx vite-bundle-analyzer dist --open",
    "lighthouse": "npx lighthouse https://prilabsa-website-2025-solaria-agency.netlify.app/ --output=json --output-path=lighthouse-report.json"
  },
  "dependencies": {
    "@dr.pogodin/react-helmet": "^3.0.2",
    "@emailjs/browser": "^4.4.1",
    "@emmett.deen/linear-mcp-server": "^1.0.0",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@react-pdf/renderer": "^4.3.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/postcss": "^4.1.10",
    "@tanstack/react-query": "^5.17.0",
    "@types/react-modal": "^3.16.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-slick": "^0.23.13",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.16",
    "csv-parser": "^3.2.0",
    "glob": "^11.0.3",
    "gray-matter": "^4.0.3",
    "i18next": "^25.3.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.525.0",
    "postcss": "^8.4.34",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.6.1",
    "react-leaflet": "^5.0.0",
    "react-markdown": "^10.1.0",
    "react-modal": "^3.16.3",
    "react-pageflip": "^2.0.3",
    "react-pdf": "^10.0.1",
    "react-router-dom": "^7.7.0",
    "react-slick": "^0.30.3",
    "react-type-animation": "^3.2.0",
    "remark-gfm": "^4.0.1",
    "slick-carousel": "^1.8.1",
    "swiper": "^11.2.10",
    "swr": "^2.3.6",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.3.3",
    "vite": "^6.3.6",
    "vitest": "^3.2.4",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.11.7",
    "@cloudflare/workers-types": "^4.20250726.0",
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.53.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__traverse": "^7.28.0",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitest/coverage-v8": "^3.2.4",
    "basic-ftp": "^5.0.5",
    "cypress": "^14.4.1",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "playwright": "^1.53.0",
    "puppeteer": "^24.10.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "sharp": "^0.34.3",
    "terser": "^5.43.1",
    "tsx": "^4.20.1",
    "typescript-eslint": "^8.20.0",
    "vite-plugin-compression": "^0.5.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "wrangler": "^4.26.0"
  }
}
